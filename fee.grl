rule NewUserFee "No fee for user with trade count less then 5" salience 8 {
    when
        InputData.TradeCount < 5
    then
        OutputData.Fee = 0;
        OutputData.Type = "percent";
        Retract("NewUserFee");
}

rule LowAmountFixFee "Fixed fee of Rs. 5 if amount is less than 1000" salience 9 {
    when
        InputData.TradeRequestAmountINR < 1000
    then
        OutputData.Fee = 5;
        OutputData.Type = "inr";
        Retract("LowAmountFixFee");
}